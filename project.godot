; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="SA2 Archipelago Slot QuickSwitcher"
run/main_scene="res://SingleScreenLayout.tscn"
config/icon="res://icon.png"

[autoload]

ProfileStorageDirectoryProvider="*res://Singletons/ProfileStorageDirectoryProvider/ProfileStorageDirectoryProvider.cs"
ProfileChangedSignalProvider="*res://Singletons/ProfileChangedSignalProvider/ProfileChangedSignalProvider.cs"
ProfileSelectedSignalProvider="*res://Singletons/ProfileSelectedSignalProvider/ProfileSelectedSignalProvider.cs"
LayoutSwitcher="*res://Singletons/LayoutSwitcher/LayoutSwitcher.tscn"

[display]

window/size/width=640
window/size/height=512
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[mono]

project/assembly_name="SA2 Archipelago Slot QuickSwitcher"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
